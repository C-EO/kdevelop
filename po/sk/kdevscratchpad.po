msgctxt "@title:window"
msgid "Scratchpad"
msgstr "Zápisník"

#: scratchpad.cpp:141 scratchpad.cpp:220 scratchpadjob.cpp:33
#, kde-format
msgctxt "prefix to distinguish scratch tabs"
msgid "scratch:%1"
msgstr "scratch:%1"

#: scratchpad.cpp:168
#, kde-format
msgid "Failed to remove scratch: %1"
msgstr "Nepodarilo sa odstrániť scratch: %1"

#: scratchpad.cpp:175
#, kde-format
msgid "Failed to create scratch: Name already in use"
msgstr "Nepodarilo sa vytvoriť scratch: Názov sa už používa"

#: scratchpad.cpp:187
#, kde-format
msgid "Failed to create new scratch"
msgstr "Nepodarilo sa vytvoriť nový scratch"

#: scratchpad.cpp:196
#, kde-format
msgid "Failed to rename scratch: Names must not include path separator"
msgstr "Nepodarilo sa spustiť scratch: Názvy nemôžu obsahovať medzery"

#: scratchpad.cpp:226
#, kde-format
msgid "Failed renaming scratch."
msgstr "Nepodarilo sa premenovať scratch."

#: scratchpadjob.cpp:57
#, kde-format
msgid "Running %1..."
msgstr "Beží %1..."

#: scratchpadjob.cpp:73
#, kde-format
msgid "Process finished with exit code %1."
msgstr "Proces bol ukončený výstupným kódom %1."

#: scratchpadjob.cpp:80
#, kde-format
msgid "Failed to run scratch: %1"
msgstr "Nepodarilo sa spustiť scratch: %1"

#: scratchpadview.cpp:100
#, kde-format
msgid ""
"Scratchpad lets you quickly run and experiment with code without a full "
"project, and even store todos. Create a new scratch to start."
msgstr ""
"Scratchpad vám umožňuje rýchlo spustiť a experimentovať s kódom bez úplného "
"projektu a dokonca ukladať úlohy. Ak chcete začať, vytvorte nový Scratch."

#: scratchpadview.cpp:114
#, kde-format
msgctxt "@info:tooltip"
msgid "Command to run this scratch. '$f' will expand to the scratch path."
msgstr "Príkaz na spustenie tohto scratch. $f sa rozbalí na cestu scratch."

