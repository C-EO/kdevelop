msgstr "Enigu ensaluton kaj pasvorton"

#: ghdialog.cpp:101 ghdialog.cpp:157 ghproviderwidget.cpp:44
#, kde-format
msgid "Waiting for response"
msgstr "Atendante respondon"

#: ghdialog.cpp:126
#, kde-format
msgid ""
"Authentication failed. Please try again.\n"
"\n"
"Could not create token: \"%1\"\n"
"%2"
msgstr ""
"Aŭtentigo malsukcesis. Bonvolu provi denove.\n"
"\n"
"Ne eblis krei ĵetonon: \"%1\"\n"
"%2"

#: ghdialog.cpp:129
#, kde-format
msgctxt "@title:window"
msgid "GitHub Authorization Failed"
msgstr "GitHub-rajtigo malsukcesis"

#: ghdialog.cpp:133
#, kde-format
msgid ""
"Authentication succeeded.\n"
"\n"
"Created token: \"%1\"\n"
"%2"
msgstr ""
"Aŭtentigo sukcesis.\n"
"\n"
"Kreita signo: \"%1\"\n"
"%2"

#: ghdialog.cpp:136
#, kde-format
msgctxt "@title:window"
msgid "GitHub Account Authorized"
msgstr "GitHub-Konto Rajtigita"

#: ghdialog.cpp:144
