#: makefileresolver/makefileresolver.cpp:230
#, kde-format
msgid ""
"Tried include path resolution while another resolution process was still "
"running"
msgstr ""
"Επιχειρήθηκε να συμπεριληφθεί η επίλυση διαδρομής ενώ μια άλλη διεργασία "
"επίλυσης εκτελείτο"

#: makefileresolver/makefileresolver.cpp:276
#, kde-format
msgid "Makefile is missing in folder \"%1\""
msgstr "Λείπει το Makefile στο φάκελο \"%1\""

#: makefileresolver/makefileresolver.cpp:276
#, kde-format
msgid "Problem while trying to resolve include paths for %1"
msgstr "Πρόβλημα στην προσπάθεια επίλυσης διαδρομών include για το %1"

#: makefileresolver/makefileresolver.cpp:310
#, kde-format
msgid "Cached: %1"
msgstr "Σε λανθάνουσα μνήμη: %1"

#: makefileresolver/makefileresolver.cpp:339
#, kde-format
msgid "Filename %1 seems to be malformed"
msgstr "Το όνομα αρχείου %1 φαίνεται να έχει λάθος μορφή"

#: makefileresolver/makefileresolver.cpp:475
#, kde-format
msgid "Failed to extract new working directory"
msgstr "Αποτυχία εξαγωγής νέου καταλόγου εργασίας"

#: makefileresolver/makefileresolver.cpp:475
#: makefileresolver/makefileresolver.cpp:504
#, kde-format
msgid "Output was: %1"
msgstr "Το αποτέλεσμα ήταν: %1"

#: makefileresolver/makefileresolver.cpp:497
#: makefileresolver/makefileresolver.cpp:500
#, kde-format
msgid "Recursive make call failed"
msgstr "Αποτυχία αναδρομικής κλήσης του make"

#: makefileresolver/makefileresolver.cpp:497
#, kde-format
msgid "The parameter string \"%1\" does not seem to be valid. Output was: %2."
msgstr "Η συμβολοσειρά \"%1\" δε φαίνεται έγκυρη. Το αποτέλεσμα ήταν: %2."

#: makefileresolver/makefileresolver.cpp:500
#, kde-format
msgid "The directory \"%1\" does not exist. Output was: %2."
msgstr "Ο κατάλογος \"%1\" δεν υπάρχει. Το αποτέλεσμα ήταν: %2."

#: makefileresolver/makefileresolver.cpp:504
#, kde-format
msgid "Malformed recursive make call"
msgstr "Λάθος μορφή αναδρομικής κλήσης του make"

#: makefileresolver/makefileresolver.cpp:516
#, kde-format
msgid "Could not extract include paths from make output"
msgstr "Αδυναμία εξαγωγής διαδρομών include από το αποτέλεσμα του make"

#: makefileresolver/makefileresolver.cpp:517
#, kde-format
msgid "Folder: \"%1\"  Command: \"%2\"  Output: \"%3\""
msgstr "Φάκελος: \"%1\"  Εντολή: \"%2\"  Αποτέλεσμα: \"%3\""

